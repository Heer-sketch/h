#include <stdio.h>

int main() {
    // Print the top horizontal border
    printf("- - - - - - - -\n");

    // Print the vertical lines and letters
    printf("|             |\n");
    printf("R             |\n");
    printf("N             |\n");
    printf("W             |\n");
    printf("|             |\n");

    // Print the bottom horizontal border
    printf("- - - - - - - -\n");

    return 0;
}
